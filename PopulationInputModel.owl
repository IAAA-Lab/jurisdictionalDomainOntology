    <!-- https://raw.github.com/stjdo/stjdo/master/PopulationInputModel.owl#date -->

    <owl:DatatypeProperty rdf:about="#date">
        <rdfs:domain rdf:resource="#JurisdictionalChange"/>
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </owl:DatatypeProperty>
    
    <!-- https://raw.github.com/stjdo/stjdo/master/PopulationInputModel.owl#destination -->

    <owl:DatatypeProperty rdf:about="#destination">
        <rdfs:domain rdf:resource="#JurisdictionalChange"/>
        <rdfs:range rdf:resource="&xsd;anyURI"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>

    <!-- https://raw.github.com/stjdo/stjdo/master/PopulationInputModel.owl#identifier -->

    <owl:DatatypeProperty rdf:about="#identifier">
        <rdfs:domain rdf:resource="#JurisdictionalDomain"/>
        <rdfs:range rdf:resource="&xsd;anyURI"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>  

    <!-- https://raw.github.com/stjdo/stjdo/master/PopulationInputModel.owl#location -->

    <owl:DatatypeProperty rdf:about="#location">
        <rdfs:range rdf:resource="http://www.opengis.net/def/dataType/OGC-SF/1.0/WKTLiteral"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#JurisdictionalDomain"/>
                    <owl:Class>
                    		<owl:intersectionOf rdf:parseType="Collection">
                    				<rdf:Description rdf:about="&rdf;Statement"/> 
                    				<owl:Class>
	                    				<owl:unionOf rdf:parseType="Collection">
		                    				<owl:Restriction>
		                							<owl:onProperty rdf:resource="&rdf;predicate"/>
		                							<owl:hasValue rdf:resource="#source"/>
		            								</owl:Restriction>
		            								<owl:Restriction>
		                							<owl:onProperty rdf:resource="&rdf;predicate"/>
		                							<owl:hasValue rdf:resource="#destination"/>
		            								</owl:Restriction>
		            							</owl:unionOf>
	            							</owl:Class>
                    		</owl:intersectionOf>
                    </owl:Class>                
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain> 
    </owl:DatatypeProperty>
    
    <!-- https://raw.github.com/stjdo/stjdo/master/PopulationInputModel.owl#name -->

    <owl:DatatypeProperty rdf:about="#name">
        <rdfs:domain rdf:resource="#JurisdictionalDomain"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    
    <!-- https://raw.github.com/stjdo/stjdo/master/PopulationInputModel.owl#part-of -->

    <owl:DatatypeProperty rdf:about="#part-of">
    	<rdfs:domain>
        <owl:Class>
          <owl:unionOf rdf:parseType="Collection">
                <rdf:Description rdf:about="#JurisdictionalDomain"/>
                <rdf:Description rdf:about="#JurisdictionalChange"/>
          </owl:unionOf>
        </owl:Class> 
      </rdfs:domain> 
      
      <rdfs:range>      
          <owl:unionOf rdf:parseType="Collection">
                <rdf:Description rdf:about="&xsd;anyURI"/>
                <rdf:Description rdf:about="&xsd;string"/>
          </owl:unionOf>
       </rdfs:range> 
    </owl:DatatypeProperty>
    
    <!-- https://raw.github.com/stjdo/stjdo/master/PopulationInputModel.owl#source -->

    <owl:DatatypeProperty rdf:about="#source">
        <rdfs:domain rdf:resource="#JurisdictionalChange"/>
        <rdfs:range>      
          <owl:unionOf rdf:parseType="Collection">
                <rdf:Description rdf:about="&xsd;anyURI"/>
                <rdf:Description rdf:about="&xsd;string"/>
          </owl:unionOf>
       </rdfs:range> 
    </owl:DatatypeProperty>
    
    <!-- https://raw.github.com/stjdo/stjdo/master/PopulationInputModel.owl#type -->

    <owl:DatatypeProperty rdf:about="#type">
        <rdfs:domain>
        <owl:Class>
          <owl:unionOf rdf:parseType="Collection">
                <rdf:Description rdf:about="#JurisdictionalDomain"/>
                <rdf:Description rdf:about="#JurisdictionalChange"/>
          </owl:unionOf>
        </owl:Class>
        </rdfs:domain>
    </owl:DatatypeProperty>
    
