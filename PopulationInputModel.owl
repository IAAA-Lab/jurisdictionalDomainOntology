    <!-- https://raw.github.com/stjdo/stjdo/master/PopulationInputModel.owl#JurisdictionalChange -->

    <owl:Class rdf:about="#JurisdictionalChange"/>
    
    <!-- https://raw.github.com/stjdo/stjdo/master/PopulationInputModel.owl#JurisdictionalDomain -->

    <owl:Class rdf:about="#JurisdictionalDomain"/>

  
